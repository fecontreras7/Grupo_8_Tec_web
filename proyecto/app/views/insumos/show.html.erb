<p id="notice"><%= notice %></p>
<div class="offset4">
	<h2>Detalle Insumo</h2>
	<p>
	  <b>Nombre:</b>
	  <%= @insumo.nombre %>
	</p>

	<p>
	  <b>Servicio:</b>
	  <%= Servicio.find(@insumo.servicio_id).nombre  %>
	</p>

	<p>
	  <b>Costo:</b>
	  <%= @insumo.costo %>
	</p>


	<div class="btn btn-primary"> <%= link_to 'Editar', edit_insumo_path(@insumo) %> </div>
	<div class="btn btn-danger"> <%= link_to 'Volver', insumos_path %> </div>
	
</div>
